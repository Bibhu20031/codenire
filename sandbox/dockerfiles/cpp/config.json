      "IsDefault": true,
      "Name": "C++17",
      "CompileCmd": "g++ -std=c++17 $(find . -name '*.cpp') -o main ",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.cpp"
      }
    },

    "c++14": {
