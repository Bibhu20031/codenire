{
  "Template": "golang_1_23",
  "Groups": ["go"],
  "Workdir": "/app",
  "ContainerOptions": {
    "CompileTTL": 30,
    "RunTTL": 5,
    "MemoryLimit": 524288000
  },

  "Actions": {
    "golang1.23": {
      "Id": "golang1.23",
      "Name": "Golang 1.23",
      "CompileCmd": "go build -o main .",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.go"
      },
      "DefaultFiles": {
        "go.mod": "module play\n"
      }
    }
  }
}
