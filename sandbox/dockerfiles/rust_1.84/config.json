{
  "Template": "rust_1.84",
  "Groups": ["rust"],
  "Workdir": "/app",
  "ContainerOptions": {
    "CompileTTL": 30,
    "RunTTL": 5,
    "MemoryLimit": 524288000
  },

  "Actions": {
    "snippet": {
      "Id": "rust",
      "Name": "Rust 1.84",
      "CompileCmd": "cargo -v",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "Enable": true,
        "SourceFile": "main.rs"
      },
      "DefaultFiles": {

      }
    }
  }
}
